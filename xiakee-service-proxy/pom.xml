<?xml version="1.0"?>
<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<artifactId>xiakee</artifactId>
		<groupId>com.xiakee</groupId>
		<version>1.0-SNAPSHOT</version>
	</parent>
	<groupId>com.xiakee</groupId>
	<artifactId>xiakee-service-proxy</artifactId>
	<version>1.0-SNAPSHOT</version>
	<name>xiakee-service-proxy</name>
	<url>http://xiakee.com</url>

	<dependencies>
		<dependency>
			<groupId>com.xiakee</groupId>
			<artifactId>xiakee-service</artifactId>
			<version>1.0-SNAPSHOT</version>
		</dependency>
	</dependencies>

	<build>
		<finalName>xiakee-service-proxy</finalName>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
